library("MSnbase")



f <- "./data/mulvey2015.csv"
e <- 2:19 ## colums with reps - quantitative data (see previous script)

m <- readMSnSet2(f, ecol=e, fnames = 1)
m
m2 <- m[1:10, 1:5]
m2

exprs(m)  ##df with expression values
fData(m)  ##df with featuredata

sampleNames(m)
featureNames(m)

featureNames(m) <- fData(m)$FeatureNames
featureNames(m)
exprs(m)[1:5, 1:3]


exprs(m2)
fData(m2)
sampleNames(m2)
featureNames(m2)


